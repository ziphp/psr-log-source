{
    "name": "ziphp/psr-log-source",
    "description": "A PSR LoggerInterface implementation that routes to the Yii logger",
    "keywords": [
        "yii2",
        "symfony",
        "loggerinterface",
        "psr-3"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/ziphp/psr-log-source/issues",
        "source": "https://github.com/ziphp/psr-log-source"
    },
    "authors": [
        {
            "name": "Sam Mousa",
            "email": "sam@mousa.nl"
        }
    ],
    "require": {
        "php": ">=8",
        "ziphp/ziphp": ">=5.0.0",
        "psr/log": "^3"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.10",
        "symplify/easy-coding-standard": "^10.1",
        "vimeo/psalm": "^4.22",
        "captainhook/plugin-composer": "^5.3",
        "phpstan/phpstan": "^1.8",
        "ramsey/conventional-commits": "^1.3"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "yii\\psr\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "vendor/ziphp/ziphp/framework/Yii.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.0.x-dev"
        }
    },
    "provide": {
        "psr/log-implementation": "3.0"
    },
    "config": {
        "allow-plugins": {
            "ziphp/zipper": true,
            "captainhook/plugin-composer": true
        }
    }
}
